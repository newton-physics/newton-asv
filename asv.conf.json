{
  "version": 1,
  "project": "newton",
  "project_url": "https://github.com/newton-physics/newton",
  "repo": "https://github.com/newton-physics/newton",
  "branches": ["main"],
  "dvcs": "git",
  "environment_type": "virtualenv",
  "show_commit_url": "https://github.com/newton-physics/newton/commit/",
  "env_dir": "env",
  "results_dir": "results",
  "html_dir": "html",
  "build_cache_size": 20,
  "default_benchmark_timeout": 600,
  "build_command": ["python -m build --wheel -o {build_cache_dir} {build_dir}"],
  "install_command": [
    "python -m pip install -U numpy",
    "python -m pip install -U --pre warp-lang==1.9.0.dev20250825 --index-url=https://pypi.nvidia.com/",
    "python -m pip install -U --pre mujoco==3.3.6.dev802089588 -f https://py.mujoco.org/",
    "python -m pip install -U torch==2.7.1+cu128 --index-url https://download.pytorch.org/whl/cu128",
    "python -m pip install git+https://github.com/google-deepmind/mujoco_warp@64bb938f87b6aaf840753ef9ddc198db655020c0",
    "in-dir={env_dir} python -m pip install {wheel_file}[dev]"
  ]
}
